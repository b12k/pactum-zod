{
  "name": "pactum-zod",
  "version": "1.0.0",
  "description": "Contract testing boilerplate using PactumJS and Zod",
  "keywords": [],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "run-s _test:*",
    "qc": "run-p _qc:*",
    "fix": "run-p _fix:*",
    "build": "run-s _build:*",
    "prepare": "run-p _prepare:*",
    "_test:wait": "env-cmd -i wait-on {{BASE_URL}} -t {{WAIT_TIMEOUT}}",
    "_test:run": "env-cmd jest",
    "_qc:eslint": "eslint . --ignore-path .gitignore",
    "_qc:typecheck": "tsc --noEmit",
    "_qc:prettier": "pnpm _prettier . -c",
    "_qc:npm-check-updates": "ncu -e 2",
    "_fix:prettier": "pnpm _prettier . -w",
    "_fix:eslint": "pnpm _eslint . --fix",
    "_build:clear": "shx rm -rf dist",
    "_build:types": "tsc --project tsconfig.build.json",
    "_prepare:husky": "husky install",
    "_prepare:env": "shx cp -u .env.example .env",
    "_husky:commit-msg": "commitlint",
    "_husky:pre-commit": "run-p _husky:pre-commit:*",
    "_husky:pre-commit:tsc": "pnpm _qc:typecheck",
    "_husky:pre-commit:lint-staged": "lint-staged",
    "_husky:pre-push": "branch-name-lint",
    "_eslint": "eslint --ignore-path .gitignore",
    "_prettier": "prettier --ignore-path .gitignore --ignore-unknown"
  },
  "author": "Bogdan Kolesnyk",
  "license": "MIT",
  "devDependencies": {
    "@b12k/branch-name-lint": "1.1.0",
    "@b12k/env-cmd": "10.2.0",
    "@commitlint/config-conventional": "19.1.0",
    "@swc/core": "1.4.11",
    "@swc/jest": "0.2.36",
    "@types/jest": "29.5.12",
    "@types/node": "20.12.2",
    "@typescript-eslint/eslint-plugin": "7.5.0",
    "@typescript-eslint/parser": "7.5.0",
    "commitlint": "19.2.1",
    "eslint": "8.57.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "18.0.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-simple-import-sort": "12.0.0",
    "eslint-plugin-tsc": "2.0.0",
    "eslint-plugin-unicorn": "51.0.1",
    "eslint-plugin-unused-imports": "3.1.0",
    "husky": "9.0.11",
    "jest": "29.7.0",
    "lint-staged": "15.2.2",
    "npm-check-updates": "16.14.18",
    "npm-run-all": "4.1.5",
    "prettier": "3.2.5",
    "shx": "0.3.4",
    "ts-node": "10.9.2",
    "typescript": "5.4.3",
    "wait-on": "7.2.0"
  },
  "dependencies": {
    "pactum": "3.6.5",
    "zod": "3.22.4"
  }
}
